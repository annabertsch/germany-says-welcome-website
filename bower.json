    "i18next": "~1.11.0",
    "FitText.js": "jquery-fittext.js#~1.2.0",
    "wow": "WOW#~1.1.2",
    "font-awesome": "fontawesome#~4.4.0",
    "angular-leaflet-directive": "~0.9.0",
